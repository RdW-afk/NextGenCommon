# Battery
genfscon sysfs /class/power_supply/battery/battery_charging_enabled      u:object_r:sysfs_battery_supply:s0
genfscon sysfs /class/power_supply/battery/charging_enabled              u:object_r:sysfs_battery_supply:s0
genfscon sysfs /class/power_supply/battery/input_suspend                 u:object_r:sysfs_battery_supply:s0
genfscon sysfs /devices/soc/c176000.i2c/i2c-2/2-001d            u:object_r:sysfs_battery_supply:s0

# Double tap to wake
genfscon proc /tpd_gesture               u:object_r:proc_tpd_gesture:s0

# LED
genfscon sysfs /devices/soc/800f000.qcom,spmi/spmi-0/spmi0-03/800f000.qcom,spmi:qcom,pm660l@3:qcom,leds@d000/leds                 u:object_r:sysfs_graphics:s0
